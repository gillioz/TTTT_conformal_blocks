 RowBox[{
  RowBox[{
   RowBox[{"TransverseInvariant", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], "[", "n_", "]"}], ":=", 
  RowBox[{"Collect", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Product", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"z", "[", "a", "]"}], "[", 
