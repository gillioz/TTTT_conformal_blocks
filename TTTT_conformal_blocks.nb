Cell[BoxData[
 RowBox[{
  RowBox[{"IndependentCoefficients", "[", "expression_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x", ",", "NonCommutativeProducts", ",", "ScalarProducts", ",", 
      "Result"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"x", "=", 
      RowBox[{"\[Zeta]sort", "[", "expression", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"NonCommutativeProducts", "=", 
      RowBox[{
       RowBox[{"Cases", "[", 
        RowBox[{"x", ",", 
         RowBox[{"NonCommutativeMultiply", "[", "__", "]"}], ",", 
         RowBox[{"-", "1"}]}], "]"}], "//", "DeleteDuplicates"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "NonCommutativeProducts", "]"}], ">", "0"}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"IndependentCoefficients", "[", 
           RowBox[{"Coefficient", "[", 
            RowBox[{"x", ",", "y"}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"y", ",", "NonCommutativeProducts"}], "}"}]}], "]"}], "//",
         "Flatten"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"ScalarProducts", "=", 
         RowBox[{"DeleteDuplicates", "[", 
          RowBox[{"Cases", "[", 
           RowBox[{"x", ",", 
            RowBox[{"CenterDot", "[", 
             RowBox[{"_", ",", "_"}], "]"}], ",", 
            RowBox[{"-", "1"}]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"Result", "=", 
         RowBox[{
          RowBox[{"CoefficientList", "[", 
           RowBox[{"x", ",", "ScalarProducts"}], "]"}], "//", "Flatten"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Head", "[", "Result", "]"}], "===", "List"}], ",", 
          RowBox[{"DeleteCases", "[", 
           RowBox[{"Result", ",", "0"}], "]"}], ",", "Result"}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
