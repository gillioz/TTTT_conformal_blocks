Cell[BoxData[{
 RowBox[{
  RowBox[{"SolveLinearEquations", "[", 
   RowBox[{"equations_", ",", "variables_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"m", ",", "b", ",", "solution"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"m", "=", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Coefficient", "[", 
          RowBox[{"equations", ",", "x"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "variables"}], "}"}]}], "]"}], "\[Transpose]"}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"b", "=", 
      RowBox[{
       RowBox[{"-", "equations"}], "/.", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "\[Rule]", "0"}], "&"}], ",", "variables"}], "]"}]}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"solution", "=", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{"m", ",", "b"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"First", "[", "#", "]"}], "\[Rule]", 
         RowBox[{"Last", "[", "#", "]"}]}], "&"}], ",", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"variables", ",", "solution"}], "}"}], "\[Transpose]"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"FreeParameters", "[", 
