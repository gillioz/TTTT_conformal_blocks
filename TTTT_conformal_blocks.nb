Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TransverseInvariantAntiSym", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{"ContractIndices", "@@", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\[Mu]", "[", "k", "]"}], ",", 
           RowBox[{"\[Nu]", "[", "k", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "n"}], "}"}]}], "]"}], "]"}]}], ")"}], "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"NonCommutativeMultiply", "@@", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"\[Zeta]", "[", "1", "]"}], "[", 
             RowBox[{"\[Mu]", "[", "k", "]"}], "]"}], "**", 
            RowBox[{
             RowBox[{"\[Zeta]", "[", "2", "]"}], "[", 
             RowBox[{"\[Nu]", "[", "k", "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ")"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"\[Zeta]toindices", "[", "1", "]"}], "@@", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"\[Mu]", "[", "k", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ")"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[Zeta]toindices", "[", "2", "]"}], "@@", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"\[Nu]", "[", "k", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ")"}], "[", 
         RowBox[{"NonCommutativeMultiply", "@@", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\[Zeta]", "[", "1", "]"}], "\[CenterDot]", 
             RowBox[{"\[Zeta]", "[", "2", "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], "]"}], "]"}]}], "/.", 
      
      RowBox[{"{", 
       RowBox[{
        RowBox[{"d", "\[Rule]", 
         RowBox[{"d", "-", "1"}]}], ",", 
        RowBox[{
         RowBox[{"\[Eta]", "[", 
          RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], "\[Rule]", 
         RowBox[{"\[Eta]perp", "[", 
          RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}]}], "}"}]}], "//", 
     "\[Zeta]sort"}], "]"}], "/.", 
   RowBox[{"{", 
