           RowBox[{"\[Nu]", "[", "k", "]"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "n"}], "}"}]}], "]"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"ztoindices", "[", "a", "]"}], "@@", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"\[Mu]", "[", "k", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ")"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"ztoindices", "[", "b", "]"}], "@@", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"\[Nu]", "[", "k", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ")"}], "[", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"z", "[", "a", "]"}], "\[CenterDot]", 
            RowBox[{"z", "[", "b", "]"}]}], ")"}], "n"], "]"}], "]"}]}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"d", "\[Rule]", 
         RowBox[{"d", "-", "1"}]}], ",", 
        RowBox[{
         RowBox[{"\[Eta]", "[", 
          RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}], "\[Rule]", 
         RowBox[{"\[Eta]perp", "[", 
          RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}]}], "}"}]}], "//", 
     RowBox[{"(", 
      RowBox[{"ContractIndices", "@@", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"\[Mu]", "[", "k", "]"}], ",", 
