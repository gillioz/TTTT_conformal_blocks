Cell["\<\
Rules for putting non-commutative products into a canonical ordering:\
\>", "Text",
 CellChangeTimes->{{3.844228649755416*^9, 
  3.844228667274428*^9}},ExpressionUUID->"e9546532-1963-413a-b3db-\
93580c3a0ed8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Zeta]sort", "[", "expression_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"expression", "//.", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"general", " ", "rules"}], " ", "*)"}], "\[IndentingNewLine]", 
      
      RowBox[{
       RowBox[{
        RowBox[{"NonCommutativeMultiply", "[", 
         RowBox[{"L___", ",", 
          RowBox[{"x_", "+", "y_"}], ",", "R___"}], "]"}], "\[RuleDelayed]", 
        RowBox[{
         RowBox[{"NonCommutativeMultiply", "[", 
          RowBox[{"L", ",", "x", ",", "R"}], "]"}], "+", 
         RowBox[{"NonCommutativeMultiply", "[", 
          RowBox[{"L", ",", "y", ",", "R"}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"NonCommutativeMultiply", "[", 
         RowBox[{"L___", ",", 
          RowBox[{"x_", " ", "y_"}], ",", "R___"}], "]"}], "\[RuleDelayed]", 
        RowBox[{
         RowBox[{"x", " ", 
          RowBox[{"NonCommutativeMultiply", "[", 
           RowBox[{"L", ",", "y", ",", "R"}], "]"}]}], "/;", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"x", ",", "\[Zeta]"}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"NonCommutativeMultiply", "[", 
         RowBox[{"L__", ",", "x_", ",", "R___"}], "]"}], "\[RuleDelayed]", 
        RowBox[{
         RowBox[{"x", " ", 
          RowBox[{"NonCommutativeMultiply", "[", 
           RowBox[{"L", ",", "R"}], "]"}]}], "/;", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"x", ",", "\[Zeta]"}], "]"}]}]}], ",", 
       RowBox[{
        RowBox[{"NonCommutativeMultiply", "[", 
         RowBox[{"x_", ",", "R__"}], "]"}], "\[RuleDelayed]", 
        RowBox[{
         RowBox[{"x", " ", 
          RowBox[{"NonCommutativeMultiply", "[", "R", "]"}]}], "/;", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"x", ",", "\[Zeta]"}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"NonCommutativeMultiply", "[", "x_", "]"}], "\[RuleDelayed]", 
        
        RowBox[{"x", "/;", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"x", ",", "\[Zeta]"}], "]"}]}]}]}], "\[IndentingNewLine]", 
      "}"}]}], "//.", 
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "move", " ", "terms", " ", "with", " ", "only", " ", "one", " ", 
       "\[Zeta]", " ", "to", " ", "the", " ", "left"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"NonCommutativeMultiply", "[", 
        RowBox[{"L___", ",", 
         RowBox[{
