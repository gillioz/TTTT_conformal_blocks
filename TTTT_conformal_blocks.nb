Cell[BoxData[
 RowBox[{
  RowBox[{"ListOfTerms", "[", "elements_List", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "n", ",", "m", ",", "allelements", ",", "scalarproducts", ",", "result", 
      ",", "i", ",", "j"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "elements", "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"allelements", "=", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
            "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",", 
            
            RowBox[{"{", 
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
             "}"}]}], "]"}], "&"}], ",", "elements"}], "]"}], "//", 
       "Flatten"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"m", "=", 
      RowBox[{"Length", "[", "allelements", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Not", "[", 
        RowBox[{
         RowBox[{"m", ">", "0"}], "&&", 
         RowBox[{"EvenQ", "[", "m", "]"}]}], "]"}], ",", 
       RowBox[{"Return", "[", "Null", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"scalarproducts", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"DeleteCases", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Apply", "[", 
             RowBox[{"CenterDot", ",", 
              RowBox[{"DeleteDuplicates", "[", 
               RowBox[{"Subsets", "[", 
                RowBox[{"allelements", ",", 
                 RowBox[{"{", "2", "}"}]}], "]"}], "]"}], ",", 
              RowBox[{"{", "1", "}"}]}], "]"}], "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"p1", "\[CenterDot]", "p1"}], "\[Rule]", "0"}], ",", 
              RowBox[{
               RowBox[{"p2", "\[CenterDot]", "p2"}], "\[Rule]", "0"}]}], 
             "}"}]}], ",", "0"}], "]"}], "//", "DeleteDuplicates"}], "}"}], 
       "\[Transpose]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"result", "=", "scalarproducts"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<", 
        FractionBox["m", "2"]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"result", "=", 
         RowBox[{"Flatten", "[", 
          RowBox[{
           RowBox[{"Outer", "[", 
            RowBox[{"Join", ",", "scalarproducts", ",", "result", ",", "1"}], 
            "]"}], ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"result", "=", 
         RowBox[{
          RowBox[{"Sort", "/@", "result"}], "//", "DeleteDuplicates"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"j", "=", "1"}], ",", 
          RowBox[{"j", "\[LessEqual]", "n"}], ",", 
          RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{"result", "=", 
           RowBox[{"Select", "[", 
            RowBox[{"result", ",", 
             RowBox[{
              RowBox[{
               RowBox[{"Count", "[", 
                RowBox[{"#", ",", 
                 RowBox[{"elements", "\[LeftDoubleBracket]", 
                  RowBox[{"j", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
                 RowBox[{"-", "1"}]}], "]"}], "\[LessEqual]", 
               RowBox[{"elements", "\[LeftDoubleBracket]", 
                RowBox[{"j", ",", "2"}], "\[RightDoubleBracket]"}]}], "&"}]}],
             "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"FreeQ", "[", 
        RowBox[{"elements", ",", "\[Zeta]"}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Apply", "[", 
        RowBox[{"Times", ",", "result", ",", "1"}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"DeleteCases", "[", 
         RowBox[{
          RowBox[{"\[Zeta]sort", "[", 
           RowBox[{"Apply", "[", 
            RowBox[{"NonCommutativeMultiply", ",", "result", ",", "1"}], 
            "]"}], "]"}], ",", "0"}], "]"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "x_"}], "\[RuleDelayed]", "x"}], "}"}]}]}], 
      "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
